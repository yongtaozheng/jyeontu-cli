{
  "name": "jyeontu",
  "version": "1.3.9",
  "description": "有趣且可能有用的命令行工具集合",
  "main": "index.js",
  "readme": "readme.md",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/yongtaozheng/jyeontu-cli.git",
      "directory": "."
    },
    {
      "type": "git", 
      "url": "https://gitee.com/zheng_yongtao/jyeontu-cli.git",
      "directory": "."
    }
  ],
  "keywords": [
    "command",
    "cli",
    "cmd",
    "JYeontu",
    "node"
  ],
  "scripts": {
