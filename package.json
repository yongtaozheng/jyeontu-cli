    "exif": "^0.6.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.1.1",
    "html-pdf-node": "^1.0.8",
    "jimp": "^0.22.10",
    "mkdirp": "^1.0.4",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3"
