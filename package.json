    "pack": "npm pack",
    "pub": "node updateVersionAndPublish.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "test": "node test/index.js"
